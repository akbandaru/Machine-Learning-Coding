    'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-ce8d8bd2-3ac2-4a26-b146-12b5d93d23a0 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"df","repr_error":"type object 'relativedelta' has no attribute 'relativedelta'"}},"metadata":{},"execution_count":84}]},{"cell_type":"code","source":["# df[(df['day']=='Sun') | (df['day']=='Sat') | (df['day']=='Fri')]"],"metadata":{"id":"FAUGrRSTDKhR","executionInfo":{"status":"ok","timestamp":1720142263617,"user_tz":240,"elapsed":20,"user":{"displayName":"Akhilesh Bandaru","userId":"10693518212609872324"}}},"execution_count":85,"outputs":[]},{"cell_type":"code","source":["options = ['Sat','Sun']"],"metadata":{"id":"G7G_fvPWDk2f","executionInfo":{"status":"ok","timestamp":1720142263617,"user_tz":240,"elapsed":20,"user":{"displayName":"Akhilesh Bandaru","userId":"10693518212609872324"}}},"execution_count":86,"outputs":[]},{"cell_type":"code","source":["df['day'].isin(options)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ed55wwPuDoMd","executionInfo":{"status":"ok","timestamp":1720142263617,"user_tz":240,"elapsed":20,"user":{"displayName":"Akhilesh Bandaru","userId":"10693518212609872324"}},"outputId":"75ec8bc2-9cef-476c-d620-42779c1edaba"},"execution_count":87,"outputs":[{"output_type":"execute_result","data":{"text/plain":["0       True\n","1       True\n","2       True\n","3       True\n","4       True\n","       ...  \n","239     True\n","